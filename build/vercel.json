{
  "buildCommand": "npx react-scripts build",
  "outputDirectory": "build",
  "installCommand": "npm ci",
  "devCommand": "npm start",
  "env": {
    "NODE_VERSION": "20",
    "GENERATE_SOURCEMAP": "false",
    "CI": "false"
  },
  "functions": {},
  "rewrites": [
    {
      "source": "/(.*)",
      "destination": "/index.html"
    }
  ]
} 